		"openlss/lib-array2xml" : "dev-master",
		"zenddevops/webapi" : "dev-master",
		"mustangostang/spyc" : "dev-master",
		"slk/icm"            : "dev-master"
	},
	"require-dev" : {
		"phpunit/phpunit" : "4.1.*",
